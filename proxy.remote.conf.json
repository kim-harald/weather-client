{
  "/api/*": {
    "target": "https://kimharald.no",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": "https://kimharald.no/api/weather"
    }
  },
  "/remote/api/*": {
    "target": "https://gimel",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": "https://gimel/api"
    }
  }
}
